<Content type="html"><![CDATA[
<script>
function displaySearchBar (prefs) {
     var src = prefs.getString("src");
     if (src == "") {
       src = prefs.getString(".src");
     }
     if (src == "") {
       src = "en";
     }
     var dst = prefs.getString("dst");
     if (dst == "") {
       dst = prefs.getString(".dst");
     }
     var html =
         '<form target="_parent" name="search_form" method="get"' +
              ' action="http://' + src + '.' + dst + '.open-tran.eu/suggest"' +
              ' onsubmit="return formsubmit();">' +
	   '<input type="text" name="q" maxlength="450" />' +
	   '<input type="submit" value="Translate" />' +
         '<form>';
     document.write(html);
}
   displaySearchBar(new _IG_Prefs(__MODULE_ID__));
   _IG_Analytics("UA-664123-3", "/google-gadget");
</script>
 ]]>
